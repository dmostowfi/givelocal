<!-- Form my_wish start -->
<%= form_with(model: @my_wish, local: true) do |f| %>
<!-- Form my_wish end -->

  <!-- Label and input for user_id start -->
  <!-- Input for "user_id" start -->
  <%= f.hidden_field :user_id, value: current_user.id %>
  <!-- Input for "user_id" end -->
  <!-- Label and input for user_id end -->

  <!-- Label and input for gift_id start -->
  <!-- Input for gift_id start -->
  <%= f.hidden_field :gift_id, value: @gift.id %>
  <!-- Input for gift_id end -->
  <!-- Label and input for gift_id end -->

  <%= f.submit "Create my wish", class: "btn btn-block btn-outline-secondary" %>
<% end %>
